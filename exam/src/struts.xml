<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
    
   <struts>
   	<package name="combo" extends="struts-default">
   		
   		<action name="resgister" class="combo.Student_Register">
   		<result name="input">index.jsp</result>
   			<result name="success">success.jsp</result>
   			<result name="error">error.jsp</result>
   		</action>
   		
   		<action name="add_book" class="combo.Add_book">
   			<result name="book_added">add_success.jsp</result>
   			<result name="book_not_added">not_added.jsp</result>
   		</action>
   		
   		<action name="delete" class="combo.Delete_book">
   		<result name="deleted">delete_success.jsp</result>
   		<result name="not_deleted">error_delete.jsp</result>
   			
   		</action>
   		
   		
   		<action name="search" class="combo.Search_book">
   			
   		<result name="book_found">book_found.jsp</result>
   		<result name="not_found">book_not_found.jsp</result>
   		</action>
   	</package>
   
   </struts> 